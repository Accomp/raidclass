{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [ "jsdoc" ]
  },
  "source": {
    "include": [ "./source/scripts/" ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./build/docs/js/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/minami"
  }
}
